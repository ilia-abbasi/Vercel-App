{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "mission.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/mission(?:/(.*))?",
      "dest": "/mission.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}